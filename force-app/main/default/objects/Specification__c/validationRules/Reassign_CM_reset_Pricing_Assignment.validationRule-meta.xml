<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Reassign_CM_reset_Pricing_Assignment</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL( plmApproval__c,&apos;Reassign CM&apos;), 
     pricingAssignmentComplete__c =True )</errorConditionFormula>
    <errorMessage>In order to Reassign CM the Pricing Assignment Complete checkbox needs to be unchecked.</errorMessage>
</ValidationRule>
