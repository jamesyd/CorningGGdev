<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Only_Acc_Owner_Can_Save_Or_Edit_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>account__r.OwnerId &lt;&gt; $User.Id</errorConditionFormula>
    <errorDisplayField>account__c</errorDisplayField>
    <errorMessage>Only Account Owner Can Save Or Edit The Record</errorMessage>
</ValidationRule>
