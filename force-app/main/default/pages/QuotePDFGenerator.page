<apex:page standardController="Quote" extensions="QuotePDFController" action="{!GenerateQuotePDF}" renderAs="pdf" language="{!IF(ISNULL(language),'en_US',language)}" applyBodyTag="false">
    <head>
        <style type="text/css">  
            @page {
            letter portrait;
            @bottom-left { font-family: Arial Unicode MS, sans-serif; font-size: 9px; content: "{!theText.corningSellingEntityLabel__c}"; }
            @bottom-center { font-family: Arial Unicode MS, sans-serif; font-size: 9px; content: "{!$ObjectType.Quote.fields.QuoteNumber.label & " " & theQuote.quoteNumber & " - " & $ObjectType.Quote.fields.Version__c.label & " " & TEXT(theQuote.Version__c) & " ** " & theQuote.quoteTypeCode__c}"; }
            @bottom-right { font-family: Arial Unicode MS, sans-serif; font-size: 9px; content: "{!securityString} Page " counter(page) " of " counter(pages); }
            }
            
            @media print {
            #tc { 
            page-break-before: always;
            } 
            }
            table { font-family: Arial Unicode MS, sans-serif; width: 100%; border-collapse:collapse; }
            td { font-family: Arial Unicode MS, sans-serif; font-size:11px; }
            td.outlined { font-family: Arial Unicode MS, sans-serif; border:1px solid black; font-size:11px;}
            th { font-family: Arial Unicode MS, sans-serif; font-size:11px;}
            .dTableHeader { font-family: Arial Unicode MS, sans-serif; background-color: silver; border:1px solid black; text-align:center; font-size:11px;}  
            .dTableColumn { font-family: Arial Unicode MS, sans-serif; background-color: white; border:1px solid black; text-align:center; font-size:11px;}  
            .dTableColumnLeft { font-family: Arial Unicode MS, sans-serif; background-color: white; border:1px solid black; text-align:left; font-size:11px;}              
        </style>
    </head>  
