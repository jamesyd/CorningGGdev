                            <TD class="dTableColumn">
                                <apex:outputText value="{0, number, ###,###,###,###}">
                                    <apex:param value="{!item.lineItem.moq__c}"/>
                                </apex:outputText>
                            </TD>
                            <TD class="dTableColumn">
                                <apex:outputText value="{0, number, ###,###,###,##0.00}">
